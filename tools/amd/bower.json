{
  "name": "<%= pkg.name %>",
  "version": "<%= pkg.version %>",
  "homepage": "<%= pkg.homepage %>",
  "author": "<%= pkg.author %>",
  "license": "<%= pkg.license %>",
  "main": [
    "react-bootstrap.js"
  ],
  "keywords": [
    <%= _.map(pkg.keywords, function(keyword) { return '"' + keyword + '"' }).join(',\n    ')%>
  ],
  "ignore": [
    "**/.*"
  ],
  "dependencies": {
    "react": "<%= pkg.peerDependencies.react %>"
  }
}
