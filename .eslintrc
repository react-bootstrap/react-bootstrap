{
  "extends": ["eslint-config-airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ],
  "rules": {
    "constructor-super": 2,
    "comma-dangle": 0,
    "eqeqeq": [2, "allow-null"],
    "id-length": 0,
    "indent": [2, 2, { "SwitchCase": 1, "VariableDeclarator": 1 }],
    "no-eq-null": 0,
    "one-var": [2, { "initialized": "never" }],
    "prefer-const": 0,
    "no-param-reassign": 0,
    "no-this-before-super": 2,
    "babel/object-shorthand": 2,
    "react/jsx-boolean-value": 2,
    "react/jsx-no-duplicate-props": 2,
    "react/sort-comp": 0,
    "react/prop-types": [2, { "ignore": [
        "children",
        "className",
        "style",
        "bsStyle",
        "bsClass",
        "bsSize"
        ]
    }]
  }
}
