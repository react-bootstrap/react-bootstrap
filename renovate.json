{
  "extends": [
    "config:base",
    ":disablePeerDependencies",
    ":semanticCommitTypeAll(chore)",
    "group:allNonMajor",
    "schedule:weekly"
  ],
  "major": {
    "masterIssueApproval": true
  },
  "rangeStrategy": "update-lockfile",
  "semanticCommits": true,
  "packageRules": [
    {
      "depTypeList": ["devDependencies"],
      "rangeStrategy": "bump"
    }
  ]
}
