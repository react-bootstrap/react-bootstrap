{
  "extends": "dtslint/dtslint.json",
  "rules": {
    "array-type": false,
    "semicolon": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ],

    "no-unnecessary-generics": false,
    "no-unnecessary-class": false,
    "file-name-casing": false
  }
}
