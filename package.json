{
  "name": "react-bootstrap",
  "version": "0.1.0",
  "description": "Bootstrap 3 components build with React",
  "main": "index.js",
  "scripts": {
    "build": "jsxc -e .jsx src/ lib/",
    "build-watch": "jsxc -e .jsx --watch src/ lib/",
    "build-test-watch": "jsxc -e .jsx --watch test/ test-built/",
    "bundle-test": "./node_modules/.bin/browserify ./test-built/*Spec.* -o test_bundle.js",
    "bundle-test-watch": "npm run build-watch& npm run build-test-watch& ./node_modules/.bin/watchify ./test-built/*Spec.* -o test_bundle.js",
    "test-watch": "npm run bundle-test-watch& karma start karma.dev.js",
    "test": "npm run bundle-test && karma start karma.ci.js"
  },
  "keywords": [
    "react",
    "bootstrap"
  ],
  "author": "Stephen J. Collings <stevoland@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "~0.8"
  },
  "devDependencies": {
    "jsxc": "~0.1.6",
    "karma-chai": "0.0.2",
    "mocha": "~1.16.2",
    "karma-script-launcher": "~0.1.0",
    "karma-chrome-launcher": "~0.1.2",
    "karma-html2js-preprocessor": "~0.1.0",
    "karma-firefox-launcher": "~0.1.3",
    "karma-jasmine": "~0.1.5",
    "karma-coffee-preprocessor": "~0.1.2",
    "requirejs": "~2.1.9",
    "karma-requirejs": "~0.2.1",
    "karma-phantomjs-launcher": "~0.1.1",
    "karma": "~0.10.9",
    "karma-mocha": "~0.1.1",
    "browserify": "~3.19.1",
    "react": "git://github.com/stevoland/react-with-test-utils",
    "watchify": "~0.5.0",
    "envify": "~0.2.0",
    "reactify": "~0.5.0"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
