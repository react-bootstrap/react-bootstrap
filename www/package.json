    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@restart/context": "^2.1.4",
    "@restart/hooks": "^0.4.0",
    "ast-types": "^0.14.2",
    "astroturf": "^0.10.5",
    "bootstrap": "^5.0.2",
    "classnames": "^2.3.1",
    "common-tags": "^1.8.0",
    "copy-text-to-clipboard": "^3.0.1",
    "docsearch.js": "^2.6.3",
    "dom-helpers": "^5.2.1",
    "formik": "^2.2.9",
    "gatsby": "^2.32.13",
    "gatsby-plugin-astroturf": "^0.2.1",
